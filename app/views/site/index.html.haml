#carousel_wrap
	%ul#banners.jcarousel-skin-banners
		%li
			=image_tag("banner1.JPG", :width => "190px", :height => "190px")
		%li
			=image_tag("banner2.JPG", :width => "190px", :height => "190px")
		%li
