.jcarousel-banners .jcarousel-container {
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
   border-radius: 6px;
    background: #F0F6F9;
    border: 1px solid #346F97;
}

.jcarousel-banners .jcarousel-direction-rtl {
	direction: rtl;
}

.jcarousel-container-horizontal {
    width: 400px;
    height: 120px;    
    padding: 2px 2px;
}

.jcarousel-banners .jcarousel-container-vertical {
    width: 400px;
    height: 120px;
    padding: 0px 0px;
}

.jcarousel-banners .jcarousel-clip {
    overflow: hidden;
}

.jcarousel-banners .jcarousel-clip-horizontal {
    width:  120px;
    height: 120px;
}

.jcarousel-banners .jcarousel-clip-vertical {
    width:  120px;
    height: 120px;
}

.jcarousel-banners .jcarousel-item {
    width: 120px;
    height: 120px;
}

.jcarousel-banners .jcarousel-item-horizontal {
	margin-left: 0;
    margin-right: 10px;
}

.jcarousel-banners .jcarousel-direction-rtl .jcarousel-item-horizontal {
	margin-left: 10px;
    margin-right: 0;
}

.jcarousel-banners .jcarousel-item-vertical {
    margin-bottom: 10px;
}

.jcarousel-banners .jcarousel-item-placeholder {
    background: #fff;
    color: #000;
}

/**
 *  Horizontal Buttons
 */
.jcarousel-banners .jcarousel-next-horizontal {
    position: absolute;
    top: 43px;
    right: 5px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background: transparent url(next-horizontal.png) no-repeat 0 0;
}

.jcarousel-banners .jcarousel-direction-rtl .jcarousel-next-horizontal {
    left: 5px;
    right: auto;
    background-image: url(prev-horizontal.png);
}

.jcarousel-banners .jcarousel-next-horizontal:hover,
.jcarousel-banners .jcarousel-next-horizontal:focus {
    background-position: -32px 0;
}

.jcarousel-banners .jcarousel-next-horizontal:active {
    background-position: -64px 0;
}

.jcarousel-banners .jcarousel-next-disabled-horizontal,
.jcarousel-banners .jcarousel-next-disabled-horizontal:hover,
.jcarousel-banners .jcarousel-next-disabled-horizontal:focus,
.jcarousel-banners .jcarousel-next-disabled-horizontal:active {
    cursor: default;
    background-position: -96px 0;
}

.jcarousel-banners .jcarousel-prev-horizontal {
    position: absolute;
    top: 43px;
    left: 5px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    background: transparent url(prev-horizontal.png) no-repeat 0 0;
}

.jcarousel-banners .jcarousel-direction-rtl .jcarousel-prev-horizontal {
    left: auto;
    right: 5px;
    background-image: url(next-horizontal.png);
}

.jcarousel-banners .jcarousel-prev-horizontal:hover, 
.jcarousel-banners .jcarousel-prev-horizontal:focus {
    background-position: -32px 0;
}

.jcarousel-banners .jcarousel-prev-horizontal:active {
    background-position: -64px 0;
}

.jcarousel-banners .jcarousel-prev-disabled-horizontal,
.jcarousel-banners .jcarousel-prev-disabled-horizontal:hover,
.jcarousel-banners .jcarousel-prev-disabled-horizontal:focus,
.jcarousel-banners .jcarousel-prev-disabled-horizontal:active {
    cursor: default;
    background-position: -96px 0;
}

